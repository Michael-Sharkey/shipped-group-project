<h1>New Shipment</h1>

<%= form_with model: @shipment, local: true do |f| %>
<%= f.label :job_id %>
<%= f.select :job_id, options_from_collection_for_select(Job.all, 'id', 'name') %>
<br>
<%= f.label :origin %>:
<%= f.select :origin, ['Boston', 'New York City', 'Philadelphia', 'Houston', 'New Orleans', 'Miami', 'Los Angeles', 'San Francisco', 'Seattle', 'Norfolk', 'Portland' ]%>
<br>
<%= f.label :destination %>:
<%= f.select :destination, ['Boston', 'New York City', 'Philadelphia', 'Houston', 'New Orleans', 'Miami', 'Los Angeles', 'San Francisco', 'Seattle', 'Norfolk', 'Portland' ]%>
<br>
<%= f.label :delivery %>:
<%= f.text_field :delivery %>
<br>
<%= f.label :boat %>
<%= f.select :boat_id, options_from_collection_for_select(Boat.all, 'id', 'name') %>
<br>
<%= f.submit%>
<% end %>
